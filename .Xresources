Xcursor.size: 32

! URxvt Color settings
URxvt*depth: 32
!URxvt*foreground: #d2bc9d
URxvt*foreground: #e5c79c
!URxvt*foreground: #f9dbb1
URxvt*background: #312f2e
URxvt*geometry: 110x50
URxvt*scrollBar: false

URxvt*font: xft:Source Code Pro:size=10
URxvt*boldFont: xft:Source Code Pro:size=10

URxvt*cursorColor: #49b4bd
URxvt*urllauncher: /usr/share/applications/chromium
URxvt*.urlLauncher: /usr/share/applications/chromium
URxvt*perl-ext-common: digital-clock,url-select,matcher,option-popup,selection,font-size
URxvt*colorUL: #59C272

URxvt*keysym.M-u: perl:url-select:select_next
URxvt*url-select.launcher: /usr/bin/xdg-open
URxvt*url-select.underline: true
! matcher
URxvt*url-launcher: /usr/bin/xdg-open
URxvt*matcher.button: 1

! digital clock
URxvt*digital-clock*foreground: #2C2A29
URxvt*digital-clock*background: #85b884
! resize-font - not working
URxvt.font-size.smaller: C-j
URxvt.font-size.bigger: C-k

! light calm orange
URxvt*color1: #e3bb6b

! calmer light peach
URxvt*color2: #d49062

! calm light pink
!URxvt*color3: #EE7070
URxvt*color3: #dfaa87

! light blue-green
URxvt*color5: #57ae9e
URxvt*color8: #57ae9e

! calmer light blue
URxvt*color6: #569491

! calmer darker blue
URxvt*color7: #3f7471

! pink
!URxvt*color4: #ee7a86
URxvt*color4: $85b884

! light red - files
